{
  "name": "timeCP",
  "version": "1.0",
  "description": "tools for CP",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.acmicpc.net/problem/*",
        "https://codeforces.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/neural128.png",
      "64": "icons/neural64.png",
      "32": "icons/neural32.png"
    }
  },
  "options_page": "options.html",
  "manifest_version": 2
}