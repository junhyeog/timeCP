{
  "name": "timeCP",
  "version": "1.0",
  "description": "tools for CP",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.acmicpc.net/problem/*",
        "https://codeforces.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/neural128.png",
      "64": "icons/neural64.png",
      "32": "icons/neural32.png"
    }
  },
  "commands": {
    "copy1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "Send a 'copy1' event to the extension"
    },
    "copy2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2"
      },
      "description": "Send a 'copy2' event to the extension"
    },
    "copy3": {
      "suggested_key": {
        "default": "Ctrl+Shift+3"
      },
      "description": "Send a 'copy3' event to the extension"
    }
  },
  "options_page": "options.html",
  "manifest_version": 2
}